{
  "name": "create-next-example-app",
  "version": "0.0.1",
  "scripts": {
    "dev": "next",
    "deploy-dev": "npm i && npm run build && pm2 restart budget-dev",
    "deploy-production": "sudo npm ci && npm run build && pm2 restart budget-prod",
    "build": "npm run build:nexus-typegen && npx next build",
    "start": "next start",
    "start-dev": "next start -p 4000",
    "start-production": "next start -p 5000",
    "build:nexus-typegen": "ts-node -r tsconfig-paths/register --compiler-options '{\"module\":\"CommonJS\"}' --transpile-only graphql/schema",
    "preinstall": "npx npm-force-resolutions",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "^3.3.19",
    "@prisma/client": "^2.23.0",
    "@types/mathjs": "^9.3.0",
    "@types/next-auth": "^3.13.0",
    "@types/styled-components": "^5.1.9",
    "apollo-server-micro": "^2.24.1",
    "apollo-upload-client": "^15.0.0",
    "fast-csv": "^4.3.6",
    "framer-motion": "^4.1.17",
    "graphql": "^15.5.0",
    "mathjs": "^9.4.0",
    "moment": "^2.29.1",
    "next": "^10.2.1",
    "next-auth": "^3.23.1",
    "nexus": "^1.0.0",
    "nexus-plugin-prisma": "^0.34.1",
    "npm": "^7.13.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.6.2",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.21.4",
    "@graphql-codegen/introspection": "1.18.2",
    "@graphql-codegen/typescript": "1.22.0",
    "@graphql-codegen/typescript-operations": "1.17.16",
    "@graphql-codegen/typescript-react-apollo": "2.2.4",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "prettier": "^2.3.0",
    "prisma": "^2.23.0",
    "typescript": "^4.2.4"
  },
  "resolutions": {
    "graphql-upload": "^11.0.0"
  }
}
