{
  "name": "create-next-example-app",
  "version": "0.0.1",
  "scripts": {
    "dev": "npx next dev",
    "deploy-dev": "npm i && npm run build && pm2 restart budget-dev",
    "deploy-production": "sudo npm ci && npm run build && pm2 restart budget-prod",
    "build": "npm run build:nexus-typegen && npx next build",
    "start": "next start",
    "start-dev": "next start -p 4000",
    "start-production": "next start -p 5000",
    "build:nexus-typegen": "ts-node -r tsconfig-paths/register --compiler-options '{\"module\":\"CommonJS\"}' --transpile-only graphql/schema",
    "preinstall": "npx npm-force-resolutions",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "^3.2.0",
    "@apollo/react-ssr": "^4.0.0",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@prisma/client": "^2.13.0",
    "@types/mathjs": "^6.0.5",
    "@types/next-auth": "^3.1.10",
    "@types/styled-components": "^5.1.4",
    "apollo-server-micro": "^2.18.1",
    "apollo-upload-client": "^14.1.3",
    "bcrypt": "^4.0.1",
    "cookie": "^0.4.1",
    "fast-csv": "^4.3.5",
    "framer-motion": "^2.9.5",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.10.3",
    "install": "^0.13.0",
    "isomorphic-fetch": "^2.2.1",
    "isomorphic-unfetch": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "mathjs": "^7.2.0",
    "micro": "^9.3.4",
    "next": "^10.0.0",
    "next-auth": "^3.1.0",
    "next-connect": "^0.9.0",
    "next-with-apollo": "^5.1.0",
    "nexus": "^1.0.0",
    "nexus-plugin-prisma": "^0.27.0",
    "npm": "^6.14.8",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^5.7.2",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.19.4",
    "@prisma/cli": "^2.13.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-eslint": "^9.0.0",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-airbnb-typescript": "^6.3.2",
    "eslint-config-gristow": "^1.0.16",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-wesbos": "0.0.19",
    "eslint-plugin-html": "^5.0.5",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^21.0.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-plugin-tsdoc": "^0.2.7",
    "esprima": "^4.0.1",
    "prettier": "^1.19.1",
    "typescript": "^4.0.3",
    "@graphql-codegen/typescript": "1.19.0",
    "@graphql-codegen/typescript-react-apollo": "2.2.1",
    "@graphql-codegen/typescript-operations": "1.17.12",
    "@graphql-codegen/introspection": "1.18.1"
  },
  "resolutions": {
    "graphql-upload": "^11.0.0"
  }
}